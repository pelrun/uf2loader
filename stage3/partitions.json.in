{
    "$schema": "https://raw.githubusercontent.com/raspberrypi/picotool/develop/json/schemas/partition-table-schema.json",
    "version": [1, 0],
    "unpartitioned": {
        "families": ["absolute"],
        "permissions": {
            "secure": "r",
            "nonsecure": "r",
            "bootloader": "rw"
        }
    },
    "partitions": [
        {
            "name": "App",
            "id": 1,
            "start": "8K",
            "size": "@part_size_kb@K",
            "families": ["rp2350-arm-ns", "rp2350-arm-s", "rp2350-riscv"],
            "permissions": {
                "secure": "rw",
                "nonsecure": "rw",
                "bootloader": "rw"
            }
        }
    ]
}
